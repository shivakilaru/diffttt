<% if signed_in? %>
	<h1>Listing Diffs</h1>

	<table>
	  <tr>
		<th>Name</th>
		<th>URL</th>
		<th>Div</th>
		<th></th>
		<th></th>
		<th></th>
	  </tr>

	<% @diffs.each do |diff| %>
	  <tr>
		<td><%= link_to diff.name, diff %></td>
		<td><%= diff.url %></td>
		<td><%= diff.div %></td>
	  </tr>
	<% end %>
	</table>

	<br />
<% else %>
	<script>
	window.location = "<%= escape_javascript(signin_path) %>";
	</script>
<% end %>